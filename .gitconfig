[user]
	name = 9yokuro
	email = 119095935+9yokuro@users.noreply.github.com
[core]
	editor = vim
[init]
	defaultBranch = main
[rerere]
	enabled = true
[column]
	ui = auto
[branch]
	sort = -committerdate
[color]
	ui = true
[alias]
	br = branch
	co = commit
	cl = clone
	di = diff
	diw = diff --word-diff
	hist = log --pretty=format:\"%Cgreen%h %Creset%cd %Cblue[%cn] %Creset%s%C(yellow)%d%C(reset)\" --graph --decorate --all
	llog = log --graph --name-status --pretty=format:\"%C(red)%h %C(reset)(%cd) %C(green)%an %Creset%s %C(yellow)%d%Creset\"
	me = merge
	mes = merge --squash
	ps = push
	pso = push origin
	pl = pull
	plo = pull origin
	st = status
	sts = status --short
	sw = switch
	swc = switch --create
	swm = switch main
